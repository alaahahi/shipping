{
    "name": "intellijapp/laravel-license",
    "description": "Laravel License Management System with Offline Support",
    "type": "library",
    "keywords": [
        "laravel",
        "license",
        "activation",
        "offline",
        "protection"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "IntellijApp\\License\\": "src/"
        },
        "files": [
            "src/Helpers/LicenseHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "IntellijApp\\License\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IntellijApp\\License\\LicenseServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

